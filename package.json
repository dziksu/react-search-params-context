    "test:watch": "tsdx test --passWithNoTests --watch",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "prettier:check": "prettier \"**/*.{js,ts,tsx,json,md}\" -c",
    "prettier:fix": "prettier \"**/*.{js,ts,tsx,json,md}\" -w"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "react-hook-search-query",
  "author": "Benjamin Rast",
  "module": "dist/react-hook-search-query.esm.js",
  "size-limit": [
    {
      "path": "dist/react-hook-search-query.cjs.production.min.js",
      "limit": "50 KB"
    },
    {
      "path": "dist/react-hook-search-query.esm.js",
      "limit": "50 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@testing-library/react": "^12.1.3",
    "ts-debounce": "^4.0.0",
    "use-context-selector": "^1.3.9"
  }
}
